{
  "name": "maxplorateur-server",
  "version": "0.1.1",
  "description": "find a tgvmax seat",
  "scripts": {
    "clean": "rm -fR ./node_modules ./.nyc_output ./coverage",
    "lint": "npx tslint ./src/*.ts{,x} ./src/**/*.ts{,x} --project tsconfig.json",
    "start": "npx ts-node ./src/index.ts",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test mocha -r ts-node/register ./test/**/*.test.ts",
    "cover": "nyc --reporter=lcov npm test",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benoitdemaegdt/TGVmax.git"
  },
  "author": "benoitdemaegdt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benoitdemaegdt/TGVmax/issues"
  },
  "homepage": "https://github.com/benoitdemaegdt/TGVmax#readme",
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "ajv": "^6.10.2",
    "bcryptjs": "^2.4.3",
    "config": "^3.2.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.8.1",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^5.1.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "lodash": "^4.17.15",
    "moment-timezone": "^0.5.26",
    "mongodb": "^3.3.2",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.3.0",
    "superagent": "^5.1.0"
  },
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.2",
    "@types/config": "0.0.34",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/koa": "^2.0.49",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^3.1.2",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-router": "^7.0.42",
    "@types/koa-static": "^4.0.1",
    "@types/koa__cors": "^2.2.3",
    "@types/lodash": "^4.14.138",
    "@types/mocha": "^5.2.7",
    "@types/moment-timezone": "^0.5.12",
    "@types/mongodb": "^3.3.1",
    "@types/nock": "^10.0.3",
    "@types/node": "^12.7.4",
    "@types/node-cron": "^2.0.2",
    "@types/nodemailer": "^6.2.1",
    "@types/superagent": "^4.1.3",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
    "nock": "^11.3.3",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.2"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 90,
    "lines": 90,
    "functions": 90,
    "statements": 90,
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
